/* ScrollBar vertical */
QScrollBar:vertical {
	border: 1px solid #6f80aa;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop: 0 #505c78, stop: 1 #6f80aa);
	width: 15px;
	margin: 17px 0 17px 0;
}
QScrollBar::handle:vertical {
	border: none;
	background: #c2d7e1;
	min-height: 20px;
}
QScrollBar::add-line:vertical  {
	border: 1px solid #6f80aa;
	background: #a5d3e8;
	height: 15px;
	subcontrol-position: bottom;
	subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical {
	border: 1px solid #6f80aa;
	background: #a5d3e8;
	height: 15px;
	subcontrol-position: top;
	subcontrol-origin: margin;
}
QScrollBar::up-arrow:vertical {
	background: url("styles:bluesky/up_arrow.png");
	background-repeat: none;
	left: 3px;
	top: 5px;
}
QScrollBar::down-arrow:vertical {
	background: url("styles:bluesky/down_arrow.png");
	background-repeat: none;
	left: 3px;
	top: 5px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}
/* ScrollBar horizontal */
QScrollBar:horizontal {
	border: 1px solid #6f80aa;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop: 0 #505c78, stop: 1 #6f80aa);
	height: 15px;
	margin: 0px 17px 0 17px;
}
QScrollBar::handle:horizontal {
	border: none;
	background: #c2d7e1;
	min-width: 20px;
}
QScrollBar::add-line:horizontal  {
	border: 1px solid #6f80aa;
	background: #a5d3e8;
	width: 15px;
	subcontrol-position: right;
	subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
	border: 1px solid #6f80aa;
	background: #a5d3e8;
	width: 15px;
	subcontrol-position: left;
	subcontrol-origin: margin;
}
QScrollBar::left-arrow:horizontal {
	background: url("styles:bluesky/left_arrow.png");
	background-repeat: none;
	left: 5px;
	top: 3px;
}
QScrollBar::right-arrow:horizontal {
	background: url("styles:bluesky/right_arrow.png");
	background-repeat: none;
	left: 5px;
	top: 3px;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
	background: none;
}
/* HeaderView */
QHeaderView::section {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6a96b9, stop: 1 #c2d7e1);
	color: #000;
	height: 25px;
	padding-left: 2px;
	border-left: 1px solid #88b4d5;;
	border-bottom: 1px solid #88b4d5;
	border-top: 1px solid transparent;
	border-right: 1px solid #88b4d5;
}
/*TableView*/
QTableView {
	alternate-background-color: #d4eefb;
	border: 1px solid #000;
}
QTableView {
	selection-background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop:1 #e9edef);
	selection-color: #0a057b;
	qproperty-alternatingRowColors: 1;
	border: 1px solid #88b4d5;
	gridline-color: #88b4d5;
}
QTableView::item {
	border: 1px solid transparent;
}
QTableView::item:hover {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #a5d3e8, stop: 1 #ccc);
	border: 1px solid transparent;
	color: #0a057b;
}
QTableView::item:selected:active {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #505c78, stop: 1 #6f80aa);
	color: #fff;
	border: 1px solid transparent;	
}
QTableView::item:selected:!active {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop: 1 #ccc);
	border: 1px solid transparent;
}
/*QSlider*/
QSlider {
	background: none;
	padding-top: 6px;
	padding-bottom: 6px;
}
QSlider::groove:horizontal { 
	background: qlineargradient(x1:0, y1:0, x2:0.5, y2:0.5, stop:0 #ccc, stop:1 #505c78);
	border: 1px solid transparent;
	border-radius: 3px;
	padding-right: 5px;
	padding-left: 5px;
	height: 7px;
}
QSlider::sub-page:horizontal { /*left side*/
	background: qlineargradient(x1:0.5, y1:0.5, x2:1, y2:1, stop:0 #505c78, stop:1 #ccc);
	border-bottom: 1px solid #0a057b;
	border-left: 1px solid #505c78;
	border-top: 1px solid #505c78;
	border-radius: 3px;
}
QSlider::add-page:horizontal { /*right side*/
	background: qlineargradient(x1:0, y1:0, x2:0.5, y2:0.5, stop:0 #ccc, stop:1 #505c78);
	border-bottom: 1px solid #0a057b;
	border-right: 1px solid #0a057b;
	border-top: 1px solid #505c78;
	border-radius: 3px;
}
QSlider::handle:horizontal { 
	background: qlineargradient(x1:0, y1:0, x2:0.7, y2:0.7, stop:0 #505c78, stop:1 #fff);
	border-bottom: 1px solid #0a057b;
	border-right: 1px solid #0a057b;
	border-left: 1px solid #505c78;
	border-top: 1px solid #505c78;
	border-radius: 3px;
	margin: -4px 0;
	width: 7px;
}
/* Statusbar */
QStatusBar {
	background-color: transparent;
}
