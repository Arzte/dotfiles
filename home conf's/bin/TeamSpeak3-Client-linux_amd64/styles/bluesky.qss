/* TabBar */
QTabWidget#permTabWidget::pane { 
     	border: 1px solid #505c78;
}
QTabBar {
	background-color: transparent;
}
QTabBar::tab {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6a96b9, stop: 1.0 #c2d7e1);
	border: 1px solid #0a057b;
	color: #000;
	min-width: 8ex;
	padding: 3px;
}
QTabBar::tab:selected, QTabBar::tab:hover {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #a5d3e8, stop: 1.0 #ccc);
}
QTabBar::tab:selected {
	border-color: #6a96b9;
	border-bottom-color: #6a96b9;
}
QTabBar::tab:selected:top {
	border-top-color: #0a057b;
	border-top-left-radius: 3px;
	border-top-right-radius: 3px;
	margin-top: 3px;
}
QTabBar::tab:selected:bottom {
	border-bottom-color: #0a057b;
	border-bottom-left-radius: 3px;
	border-bottom-right-radius: 3px;
	margin-bottom: 3px;
}
QTabBar::tab:!selected:top {
	border-top-color: #0a057b;
	border-top-left-radius: 3px;
	border-top-right-radius: 3px;
	margin-top: 5px;
}
QTabBar::tab:!selected:bottom {
	border-bottom-color: #0a057b;
	border-bottom-left-radius: 3px;
	border-bottom-right-radius: 3px;
	margin-bottom: 5px;
}

/* SpinBox */

QSpinBox, QDoubleSpinBox#delayPTTSpinBox, QSpinBox#uploadBWSpinBox, QSpinBox#downloadBWSpinBox, QSpinBox#reloadChannelChatsSpinBox, QSpinBox#reloadClientChatsSpinBox, QSpinBox#uploadSpeedSpinBox, QSpinBox#downloadSpeedSpinBox {
	background: #c2d7e1;
	border: 1px solid #0a057b;
	border-top-right-radius: 3px;
	border-bottom-left-radius: 3px;
	padding: 2px;
	color: #000;
	selection-background-color: none;
}
QSpinBox::disabled, QDoubleSpinBox#delayPTTSpinBox::disabled, QSpinBox#uploadBWSpinBox::disabled, QSpinBox#downloadBWSpinBox::disabled, QSpinBox#reloadChannelChatsSpinBox::disabled, QSpinBox#reloadClientChatsSpinBox::disabled, QSpinBox#uploadSpeedSpinBox::disabled, QSpinBox#downloadSpeedSpinBox::disabled {
	background:  #c2d7e1;
	border: 1px solid #0a057b;
	border-top-right-radius: 3px;
	border-bottom-left-radius: 3px;
	padding: 2px;
	color: #7b7b7b;
	selection-background-color: none;
}
QSpinBox::up-button,QDoubleSpinBox#delayPTTSpinBox::up-button {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop: 1.0 #eef5f9);
	border-bottom-left-radius: 3px;
	border: 1px solid #0a057b;
	border-bottom: 0px;
	border-right: 0px;
	border-top-right-radius: 3px;
	border-top: none;
	width: 20px;
}
QSpinBox::up-button:hover,QDoubleSpinBox#delayPTTSpinBox::up-button:hover {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #eef5f9, stop: 1.0 #c2d7e1);
}
QSpinBox::up-button:pressed,QDoubleSpinBox#delayPTTSpinBox::up-button:pressed {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #eef5f9, stop: 1.0 #c2d7e1);
}
QSpinBox::up-arrow,QDoubleSpinBox#delayPTTSpinBox::up-arrow {
	image:url("styles/bluesky/up_arrow.png");
	background-position: center center;
	background-repeat: no-repeat;
}
QSpinBox::up-arrow:disabled,QSpinBox::up-arrow:off,QDoubleSpinBox#delayPTTSpinBox::up-arrow:disabled,QDoubleSpinBox#delayPTTSpinBox::up-arrow:off {
	image: url("styles/bluesky/up_arrow.png");
}
QSpinBox::down-button,QDoubleSpinBox#delayPTTSpinBox::down-button {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #eef5f9, stop: 1.0 #c2d7e1);
	border-top-left-radius: 3px;
	border:1px solid #0a057b;
	border-bottom: 0px;
	border-right: 0px;
	width:20px;
}
QSpinBox::down-button:hover,QDoubleSpinBox#delayPTTSpinBox::down-button:hover {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop: 1.0 #eef5f9);
}
QSpinBox::down-button:pressed,QDoubleSpinBox#delayPTTSpinBox::down-button:pressed {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #eef5f9, stop: 1.0 #c2d7e1);
}
QSpinBox::down-arrow,QDoubleSpinBox#delayPTTSpinBox::down-arrow {
	image: url("styles/bluesky/down_arrow.png");
	background-position: center center;
	background-repeat: no-repeat;
}
QSpinBox::down-arrow:disabled,QSpinBox::down-arrow:off,QDoubleSpinBox#delayPTTSpinBox::down-arrow:disabled,QDoubleSpinBox#delayPTTSpinBox::down-arrow:off {
	image: url("styles/bluesky/down_arrow.png");
}
/* Tree View */
QTreeView {
	alternate-background-color: #d4eefb;
	show-decoration-selected: 1;
	border: 1px solid transparent;
	border-radius: 3px;
}
TreeView {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop:1 #eef5f9);
	qproperty-alternatingRowColors: 1;
	border: 1px solid transparent;
	border-radius: 3px;
	color:#000;
}
QTreeView::item {
	height: 16px;
	border: 0px;
}
QTreeView::item:hover {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6a96b9, stop: 1.0 #c2d7e1);
	color: #0a057b;
}
QTreeView::item:selected:active {
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop: 0 #505c78, stop: 1 #6f80aa);
	color: #c1daff;
}
QTreeView::item:selected:!active {
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop: 0 #505c78, stop: 1 #6f80aa);
	color: #fff;
}
QTreeView::branch:hover {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6a96b9, stop: 1.0 #c2d7e1);
}
/* QTreeView::branch:selected:!active -> wird vom :active übernommen */
QTreeView::branch:selected {
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop: 0 #505c78, stop: 1 #6f80aa);
}
QTreeView::branch:has-siblings:!adjoins-item {
	border-image: url(styles:bluesky/sibling_branch.png) 0;
}
QTreeView::branch:has-siblings:adjoins-item {
	border-image: url(styles:bluesky/sibling_branch.png) 0;
}
QTreeView::branch:!has-children:!has-siblings:adjoins-item {
	border-image: url(styles:bluesky/sibling_branch.png) 0;
}
QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
	border-image: none;
	image: url(styles:bluesky/open_arrow.png);
}
QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings  {
	border-image: none;
	image: url(styles:bluesky/close_arrow.png);
}
