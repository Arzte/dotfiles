QMainWindow, QWidget, QDialog, TextBrowser, QAbstractItemView, QTextEdit {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop:1 #eef5f9);
	color:#000;
}
QWidget#MainWindowServerTabsWidget, QWidget#MainWindowChatWidget {
	background-color: transparent;
}
QWidget#scrollAreaWidgetContents, QWidget#klappWidget_1, QWidget#klappWidget_2, QWidget#klappWidget_3, QWidget#klappWidget_4, QWidget#klappWidget_5, QWidget#klappWidget_6, QWidget#klappWidget_7 {
	background-color: #c2d7e1;
}
QWidget#NewsBanner {
	color: darkBlue;
	background-color: #c2d7e1;
	border: 1px solid #0a057b;
	border-radius: 3px;
	padding-left: 5px;
}
QLabel#NewsLeftLabel {
	padding-right: 6px;
}
/* QGroupBox */
QGroupBox#filetransferGroupBox, QGroupBox#groupOptionsGroupBox, QGroupBox#adminServerGroupBox, QGroupBox#manageGroupsGroupBox, QGroupBox#assignRemoveMemberGroupBox, QGroupBox#adminClientsGroupBox, QGroupBox#manageChannelsGroupBox, QGroupBox#basicsGroupBox {
	background-color: #c2d7e1;
	border: 1px solid #fff;
	border-radius: 3px;
	margin-top: 2ex;
}
QGroupBox#filetransferGroupBox::title, QGroupBox#groupOptionsGroupBox::title, QGroupBox#adminServerGroupBox::title, QGroupBox#manageGroupsGroupBox::title, QGroupBox#assignRemoveMemberGroupBox::title, QGroupBox#adminClientsGroupBox::title, QGroupBox#manageChannelsGroupBox::title, QGroupBox#basicsGroupBox::title {
	subcontrol-origin:margin;
	padding:0 5px;
	margin-left: 8px;
	left:11px;
}	
QSplitter::handle {
     	background-color: transparent;
}
QSplitter::handle:horizontal {
     	background-color: transparent;
 	width: 2px;
}
QSplitter::handle:vertical {
     	background-color: transparent;
 	height: 2px;
}
QLineEdit {
	border: 1px solid #0a057b;
	border-radius: 3px;
	background: #d6eaf4;
}
ChatLineEdit {
	border: 1px solid #0a057b;
	border-radius: 3px;
	background: #d6eaf4;
}
/* PushButton */
QPushButton {
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop: 0 #6a96b9, stop: 1 #c2d7e1);
	border: 1px solid #0a057b;
	border-radius: 3px;
	padding-left: 14px; padding-right: 14px;
	padding-top: 3px; padding-bottom: 3px;
}
QPushButton:hover {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #a5d3e8, stop:1 #ccc);
	color: #0a057b;
}
QPushButton:pressed {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop: 1 #ccc);
}
QPushButton:disabled {
	border: 1px solid #0a057b;
	border-radius: 3px;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #ccc, stop: 1 #ddd);
	color: #7b7b7b;
}
/* MenuBar */
QMenuBar {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6a96b9, stop: 1 #c2d7e1);
	spacing: 2px;
	color: #000;
}
QMenuBar::item {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6a96b9, stop: 1 #c2d7e1);
	color: #000;
}
QMenuBar::item:selected {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #a5d3e8, stop: 1 #e9edef);
	color: #000;
}
QMenuBar::item:pressed {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop: 1 #e9edef);
	color: #000;
}
QMenu {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #b3d1e7, stop: 1 #e1eef8);
	color: #000;
	margin: 1px;
}
QMenu::item {
	background: transparent;
	padding: 3px;
	padding-left: 20px;
	padding-right: 10px;
}
QMenu::item:disabled {
	color: #7b7b7b;
}
QMenu::item:selected {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop: 1 #e9edef);
	color: #0a057b;
}
QMenu::separator {
	height:1px;
	margin-bottom: 2px;
	margin-top: 2px;
	background: #0a057b;
}
/*QMenuIndicator*/
QMenu::indicator:checked {
	image: url("styles:bluesky/menu_check.png");
}
QMenu::indicator:non-exclusive:checked {
	image: url("styles:bluesky/menu_check.png");
}
/* Combobox */
QComboBox {
	border:1px solid #0a057b;
	border-radius: 3px;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #6a96b9, stop:1 #c2d7e1);
	color: #000;
	padding: 2px;
	selection-background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #616b70, stop:1 #6a96b9);
	color: #000;
}
QComboBox:!editable:on, QComboBox::drop-down:editable:on {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #d2f0ff, stop:1 #ccc);
}
QComboBox:on {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #eee, stop:1 #ccc);
}
QComboBox QAbstractItemView {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop: 1 #eef5f9);
	border: 1px solid #0a057b;
}
QComboBox#utcLocalComboBox {
	padding-left: 14px;
	padding-right: 7px;
}
/* Listview */
QListView {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop: 1 #eef5f9);
	color:#000;
	selection-background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop:1 #e9edef);
	selection-color: #0a057b;
	show-decoration-selected: 1;
	border: 1px solid #0a057b;
}
QListView::item:hover {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #a5d3e8, stop: 1 #ccc);
	border: none;
	border-radius: none;
}
QListView::item:selected:active {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #505c78, stop: 1 #6f80aa);
	color: #fff;
	border: none;
}
QListView::item:selected:!active {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop: 1 #ccc);
	color: #0a057b;
	border: none;
}
QListWidget#identitiesListWidget {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop: 1 #eef5f9);
	color:#000;
}
QListView#fileListView::item:hover {
	background: transparent;
}
QListView#fileListView::item:selected {
	background: transparent;
}
/* Toolbar */
QToolBar {
	border: transparent;
	background: none;
	spacing: 5px;
}
/* ToolButton */
